  margin-top: 18rpx;
  /* border: 1px saddlebrown solid */
}

.box-nav-one .box-nav-one-right{
  width:219rpx;
  height: auto;
  /* background: yellow;    */
  position: absolute;
  right:34rpx;
  top:30rpx;
  text-align: left
}

.box-nav-one .box-nav-one-right .active{
  width: 100%;
  text-align: center;
  font-size: 26rpx;
