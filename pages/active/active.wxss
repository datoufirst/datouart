.content{
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}  

.nav{
      width:100%;
      height: 82rpx;
      background: #fff;
      opacity: 0.9
    }
    .navbar{  
      width:92%;
      margin:0 auto;
      flex: none;  
      display: flex;  
      font-size: 28rpx; 
      font-weight: bold;
      color: #855a30;
    }  
    .navbar .item{  
      flex: auto;  
      text-align: center;  
      line-height: 82rpx; 
    }  
    .navbar .item.active{  
      position: relative; 
      color: #855a30; 
    }  
    .navbar .item.active:after{  
      content: "";  
      display: block;  
      position: absolute;  
      bottom:10rpx;  
      left: 128rpx;  
      right: 0;
      width:80rpx; 
      height: 4rpx;  
      background: #855a30;   
    }
     .one{
      width: 92%;
      height: 690rpx;
      margin-top: 34rpx;
