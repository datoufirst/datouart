.box .box-nav{
  width:92%;
  height: 100%;
  margin: 0 auto;
}
.box-nav-one{
  position: relative;
  margin-bottom: 30rpx;
}
.box-nav-one image{
  width:692rpx;
  height: 301rpx;
  border-radius: 12rpx;
}
.box-nav-one-left{
  width:402rpx;
  height: 267rpx;
  position: absolute;
  left:0rpx;
  top:0rpx;
}

.box-nav-one-left image{
  width:402rpx;
  height: 267rpx;
  border-radius: 12rpx;
  float: left;
